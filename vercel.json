{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "bot-backend/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/bot-backend/api/$1"
    },
    {
      "src": "/health",
      "dest": "/bot-backend/api/health"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}